{% extends 'base.html.twig' %}
{% block body %}
    <section>
        {% for wish in wishes %}
            <ul>
                <li><a href="{{ path('wish_details',{'id':wish.id}) }}">{{ wish.title }} de {{ wish.author }} ({{ wish.category.name }})</a></li>
            </ul>


        {% endfor %}
    </section>
{% endblock %}

{% block title %}
    {{ parent() }} | Listes des souhaits
{% endblock %}